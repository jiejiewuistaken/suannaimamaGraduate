    "def visualize_residual_diagnostics(comparison_df, model_predictions, product_name, output_dir, config):\n",
    "    \"\"\"可视化残差诊断：自相关和分布 - 配置驱动版本\"\"\"\n",
    "    \n",
    "    fig, axes = plt.subplots(2, 2, figsize=(16, 12))\n",
    "    \n",
    "    actual = comparison_df['actual_demand'].values\n",
    "    \n",
    "    # 动态选择模型：优先Ensemble，然后是主要模型\n",
    "    baseline_models = ['LinearRegression'] + [m for m in model_predictions.keys() if '移动平均' in m]\n",
    "    \n",
    "    priority_order = ['Improved_Ensemble', 'XGBoost', 'RandomForest', 'LightGBM', 'LSTM', 'QRF', 'SVR']\n",
    "    available_models = []\n",
    "    \n",
    "    for model in priority_order:\n",
    "        if model in model_predictions.keys() and model not in baseline_models:\n",
    "            available_models.append(model)\n",
    "            if len(available_models) == 7:\n",
    "                break\n",
    "    \n",
    "    # 如果不够4个，补充其他非基线模型\n",
    "    if len(available_models) < 4:\n",
    "        other_models = [m for m in model_predictions.keys() \n",
    "                       if m not in baseline_models and m not in available_models]\n",
    "        available_models.extend(other_models[:4-len(available_models)])\n",
    "    \n",
    "    colors = ['red', 'blue', 'green', 'purple']\n",
    "    \n",
    "    for idx, model in enumerate(available_models[:4]):\n",
