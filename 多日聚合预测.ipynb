    "def visualize_config_comparison(df, base_dir):\n",
    "    \"\"\"生成配置对比可视化 - 分别对比LinearRegression和移动平均基线\"\"\"\n",
    "    \n",
    "    products = df['产品'].unique()\n",
    "    n_products = len(products)\n",
    "    \n",
    "    # 定义要分析的指标\n",
    "    metrics_to_plot = {\n",
    "        'WMAPE (%)': {'lower_is_better': True, 'format': '{:.1f}%', 'ylabel': 'WMAPE (%)'},\n",
    "        'MAE': {'lower_is_better': True, 'format': '{:.0f}', 'ylabel': 'MAE'},\n",
    "        'RMSE': {'lower_is_better': True, 'format': '{:.0f}', 'ylabel': 'RMSE'},\n",
    "        'R²': {'lower_is_better': False, 'format': '{:.3f}', 'ylabel': 'R²'}\n",
    "    }\n",
    "    \n",
    "    # 两种基线\n",
    "    baseline_groups = {\n",
    "        'LinearRegression': ['LinearRegression'],\n",
    "        '移动平均': [name for name in df['模型'].unique() if '移动平均' in name]\n",
    "    }\n",
    "    \n",
    "    for metric_name, metric_info in metrics_to_plot.items():\n",
    "        if metric_name not in df.columns:\n",
    "            print(f\"  跳过指标 {metric_name}（数据中不存在）\")\n",
    "            continue\n",
    "        \n",
    "        print(f\"\\n  生成 {metric_name} 对比图...\")\n",
    "        \n",
    "        for baseline_name, baseline_models in baseline_groups.items():\n",
    "            if not any(model in df['模型'].values for model in baseline_models):\n",
    "                print(f\"    跳过基线 {baseline_name}（数据中不存在）\")\n",
    "                continue\n",
    "            \n",
    "            # ========== 修改：增加图表高度，为改进标注预留空间 ==========\n",
    "            fig, axes = plt.subplots(1, n_products, figsize=(8*n_products, 7))  # 7而不是6\n",
    "            if n_products == 1:\n",
    "                axes = [axes]\n",
    "            \n",
    "            for idx, product in enumerate(products):\n",
    "                product_df = df[df['产品'] == product]\n",
    "                config_codes = sorted(product_df['配置代码'].unique())\n",
    "                \n",
    "                best_values = []\n",
    "                baseline_values = []\n",
    "                improvement_pcts = []\n",
    "                \n",
    "                for config_code in config_codes:\n",
    "                    config_data = product_df[product_df['配置代码'] == config_code]\n",
    "                    \n",
    "                    baseline_data = config_data[config_data['模型'].isin(baseline_models)]\n",
    "                    non_baseline_data = config_data[~config_data['模型'].isin(baseline_models)]\n",
    "                    \n",
    "                    if len(non_baseline_data) > 0:\n",
    "                        if metric_info['lower_is_better']:\n",
    "                            best_value = non_baseline_data[metric_name].min()\n",
    "                        else:\n",
    "                            best_value = non_baseline_data[metric_name].max()\n",
    "                        best_values.append(best_value)\n",
    "                    else:\n",
    "                        best_values.append(np.nan)\n",
    "                        best_value = np.nan\n",
    "                    \n",
    "                    if len(baseline_data) > 0:\n",
    "                        if metric_info['lower_is_better']:\n",
    "                            baseline_value = baseline_data[metric_name].min()\n",
    "                        else:\n",
    "                            baseline_value = baseline_data[metric_name].max()\n",
    "                        baseline_values.append(baseline_value)\n",
    "                    else:\n",
    "                        baseline_values.append(np.nan)\n",
    "                        baseline_value = np.nan\n",
    "                    \n",
    "                    if not np.isnan(best_value) and not np.isnan(baseline_value) and baseline_value != 0:\n",
    "                        if metric_info['lower_is_better']:\n",
    "                            improvement = (baseline_value - best_value) / abs(baseline_value) * 100\n",
    "                        else:\n",
    "                            improvement = (best_value - baseline_value) / abs(baseline_value) * 100\n",
    "                        improvement_pcts.append(improvement)\n",
    "                    else:\n",
    "                        improvement_pcts.append(0)\n",
    "                \n",
    "                x = np.arange(len(config_codes))\n",
    "                width = 0.35\n",
    "                \n",
    "                bars1 = axes[idx].bar(x - width/2, best_values, width, \n",
    "                                     label='非基线最优', color='#3498db', alpha=0.8)\n",
    "                \n",
    "                bars2 = axes[idx].bar(x + width/2, baseline_values, width,\n",
    "                                     label=f'{baseline_name}基线', color='#95a5a6', alpha=0.6)\n",
    "                \n",
    "                # ========== 修改：计算固定的标注位置 ==========\n",
    "                all_values = [v for v in best_values + baseline_values if not np.isnan(v)]\n",
    "                if all_values:\n",
    "                    y_min = min(all_values)\n",
    "                    y_max = max(all_values)\n",
    "                    y_range = y_max - y_min\n",
    "                    \n",
    "                    # 为改进标注预留固定空间\n",
    "                    axes[idx].set_ylim(y_min - 0.05 * y_range, y_max + 0.35 * y_range)\n",
    "                    \n",
    "                    # 改进标注的固定Y坐标（在最高柱子上方10%处）\n",
    "                    label_y = y_max + 0.15 * y_range\n",
    "                \n",
    "                # 在柱子上标注数值\n",
    "                for i, (bar1, bar2, val1, val2, improv) in enumerate(zip(bars1, bars2, best_values, baseline_values, improvement_pcts)):\n",
    "                    if not np.isnan(val1):\n",
    "                        height1 = bar1.get_height()\n",
    "                        axes[idx].text(bar1.get_x() + bar1.get_width()/2., height1,\n",
    "                                      metric_info['format'].format(val1),\n",
    "                                      ha='center', va='bottom', fontsize=9, fontweight='bold')\n",
    "                    \n",
    "                    if not np.isnan(val2):\n",
    "                        height2 = bar2.get_height()\n",
    "                        axes[idx].text(bar2.get_x() + bar2.get_width()/2., height2,\n",
    "                                      metric_info['format'].format(val2),\n",
    "                                      ha='center', va='bottom', fontsize=9, style='italic')\n",
    "                    \n",
    "                    # ========== 改进标注固定在顶部 ==========\n",
    "                    if not np.isnan(val1) and not np.isnan(val2):\n",
    "                        if improv > 0.5:\n",
    "                            color = 'green'\n",
    "                            sign = '↑'\n",
    "                        elif improv < -0.5:\n",
    "                            color = 'red'\n",
    "                            sign = '↓'\n",
    "                        else:\n",
    "                            color = 'gray'\n",
    "                            sign = '≈'\n",
    "                        \n",
    "                        # 使用固定的Y坐标\n",
    "                        axes[idx].text(x[i], label_y,\n",
    "                                      f'{sign}{abs(improv):.1f}%',\n",
    "                                      ha='center', va='center', fontsize=10,\n",
    "                                      color=color, fontweight='bold',\n",
    "                                      bbox=dict(boxstyle='round,pad=0.4', facecolor='white', \n",
    "                                              edgecolor=color, alpha=0.9, linewidth=2))\n",
    "                \n",
    "                axes[idx].set_title(f'{product}\\n{metric_name}对比（vs {baseline_name}）', \n",
    "                                   fontsize=12, fontweight='bold', pad=15)\n",
    "                axes[idx].set_xlabel('配置', fontsize=10)\n",
    "                axes[idx].set_ylabel(metric_info['ylabel'], fontsize=10)\n",
    "                axes[idx].set_xticks(x)\n",
    "                axes[idx].set_xticklabels(config_codes, rotation=45, ha='right')\n",
    "                axes[idx].legend(loc='upper right', fontsize=9)\n",
    "                axes[idx].grid(True, alpha=0.3, axis='y', linestyle='--')\n",
    "                \n",
    "                if not metric_info['lower_is_better'] and y_min < 0:\n",
    "                    axes[idx].axhline(0, color='black', linestyle='-', linewidth=0.5, alpha=0.3)\n",
    "            \n",
    "            plt.suptitle(f'跨配置 {metric_name} 对比（基线={baseline_name}，绿色↑=优于基线，红色↓=劣于基线）', \n",
    "                        fontsize=14, fontweight='bold', y=0.98)\n",
    "            plt.tight_layout(rect=[0, 0, 1, 0.96])\n",
    "            \n",
    "            plot_path = os.path.join(base_dir, \n",
    "                f\"跨配置对比_{metric_name.replace(' ', '_').replace('(%)', '')}_{baseline_name}_可视化.png\")\n",
    "            plt.savefig(plot_path, dpi=300, bbox_inches='tight')\n",
    "            plt.close()\n",
    "            print(f\"    {metric_name} vs {baseline_name} 对比图已保存\")\n",
    "    \n",
    "    print(f\"\\n  所有指标对比可视化已完成\")\n",
    "\n",
    "\n",
    "def visualize_comprehensive_comparison(df, base_dir):\n",
    "    \"\"\"生成综合对比热力图：显示所有指标相对基线的改进\"\"\"\n",
    "    \n",
    "    # 动态识别基线模型\n",
    "    all_models = df['模型'].unique()\n",
    "    baseline_groups = {\n",
    "        'LinearRegression': ['LinearRegression'],\n",
    "        '移动平均': [m for m in all_models if '移动平均' in m]\n",
    "    }\n",
    "    \n",
    "    products = df['产品'].unique()\n",
    "    \n",
    "    # ========== 修改：删除MAPE，添加RMSE ==========\n",
    "    metrics_info = {\n",
    "        'WMAPE (%)': True,  # lower is better\n",
    "        'MAE': True,\n",
    "        'RMSE': True,  # ✅ 添加RMSE\n",
    "        # 'MAPE (%)': True,  # ✅ 删除MAPE\n",
    "        'R²': False\n",
    "    }\n",
    "    \n",
    "    # ========== 为每种基线生成热力图 ==========\n",
    "    for baseline_name, baseline_models in baseline_groups.items():\n",
    "        if not baseline_models:\n",
    "            print(f\"  跳过基线 {baseline_name}（数据中不存在）\")\n",
    "            continue\n",
    "        \n",
    "        print(f\"\\n  生成相对{baseline_name}的综合改进热力图...\")\n",
    "        \n",
    "        for product in products:\n",
    "            product_df = df[df['产品'] == product]\n",
    "            config_codes = sorted(product_df['配置代码'].unique())\n",
    "            \n",
    "            # 构建改进矩阵\n",
    "            improvement_matrix = []\n",
    "            \n",
    "            for config in config_codes:\n",
    "                config_data = product_df[product_df['配置代码'] == config]\n",
    "                improvements = []\n",
    "                \n",
    "                for metric, lower_is_better in metrics_info.items():\n",
    "                    if metric not in df.columns:\n",
    "                        improvements.append(0)\n",
    "                        continue\n",
    "                    \n",
    "                    # ========== 修改：明确分离基线和非基线 ==========\n",
    "                    baseline_data = config_data[config_data['模型'].isin(baseline_models)]\n",
    "                    non_baseline_data = config_data[~config_data['模型'].isin(baseline_models)]\n",
    "                    \n",
    "                    if len(baseline_data) > 0 and len(non_baseline_data) > 0:\n",
    "                        if lower_is_better:\n",
    "                            baseline_val = baseline_data[metric].min()\n",
    "                            best_val = non_baseline_data[metric].min()\n",
    "                            improv = (baseline_val - best_val) / abs(baseline_val) * 100\n",
    "                        else:\n",
    "                            baseline_val = baseline_data[metric].max()\n",
    "                            best_val = non_baseline_data[metric].max()\n",
    "                            improv = (best_val - baseline_val) / abs(baseline_val) * 100\n",
    "                        improvements.append(improv)\n",
    "                    else:\n",
    "                        improvements.append(0)\n",
    "                \n",
    "                improvement_matrix.append(improvements)\n",
    "            \n",
    "            # 绘制热力图\n",
    "            fig, ax = plt.subplots(figsize=(10, max(6, len(config_codes) * 0.6)))\n",
    "            \n",
    "            improvement_matrix = np.array(improvement_matrix)\n",
    "            \n",
    "            im = ax.imshow(improvement_matrix, cmap='RdYlGn', aspect='auto', \n",
    "                          vmin=-30, vmax=30, interpolation='nearest')\n",
    "            \n",
    "            ax.set_xticks(np.arange(len(metrics_info)))\n",
    "            ax.set_yticks(np.arange(len(config_codes)))\n",
    "            ax.set_xticklabels(list(metrics_info.keys()), fontsize=11)\n",
    "            ax.set_yticklabels(config_codes, fontsize=10)\n",
    "            \n",
    "            # 标注数值\n",
    "            for i in range(len(config_codes)):\n",
    "                for j in range(len(metrics_info)):\n",
    "                    value = improvement_matrix[i][j]\n",
    "                    \n",
    "                    # 根据数值大小选择文字颜色\n",
    "                    text_color = 'white' if abs(value) > 15 else 'black'\n",
    "                    \n",
    "                    # 添加符号\n",
    "                    if value > 0.5:\n",
    "                        sign = '↑'\n",
    "                    elif value < -0.5:\n",
    "                        sign = '↓'\n",
    "                    else:\n",
    "                        sign = '≈'\n",
    "                    \n",
    "                    ax.text(j, i, f'{sign}{abs(value):.1f}%',\n",
    "                           ha=\"center\", va=\"center\", \n",
    "                           color=text_color, fontsize=10, fontweight='bold')\n",
    "            \n",
    "            ax.set_title(f'{product} - 各配置相对{baseline_name}改进热力图\\n（绿色=改进，红色=退步）', \n",
    "                        fontsize=13, fontweight='bold', pad=15)\n",
    "            \n",
    "            cbar = plt.colorbar(im, ax=ax, label='改进百分比 (%)', fraction=0.046, pad=0.04)\n",
    "            cbar.ax.tick_params(labelsize=10)\n",
    "            \n",
    "            plt.tight_layout()\n",
    "            \n",
    "            # ========== 修改：文件名包含基线信息 ==========\n",
    "            heatmap_path = os.path.join(base_dir, f'{product}_综合改进热力图_{baseline_name}.png')\n",
    "            plt.savefig(heatmap_path, dpi=300, bbox_inches='tight')\n",
    "            plt.close()\n",
    "            \n",
    "        print(f\"    相对{baseline_name}的热力图已生成\")\n",
    "\n",
    "# =============================================================================\n",
    "# 批量测试脚本（可选）\n",
    "# =============================================================================\n",
    "\n",
    "def batch_test_configs():\n",
    "    \"\"\"批量测试多个配置\"\"\"\n",
    "    \n",
    "    test_configs = [\n",
    "        WindowConfig(past_window=7, future_window=7, step=7),\n",
    "        WindowConfig(past_window=15, future_window=7, step=7),\n",
    "        WindowConfig(past_window=30, future_window=7, step=7),\n",
    "        WindowConfig(past_window=15, future_window=15, step=15),\n",
    "        WindowConfig(past_window=15, future_window=15, step=1),\n",
    "        WindowConfig(past_window=15, future_window=15, step=7),\n",
    "        WindowConfig(past_window=30, future_window=15, step=7)\n",
    "    ]\n",
    "    \n",
    "    base_dir = r\"/Users/bytedance/Desktop/特征工程补充-半月版444/结果\"\n",
    "    products = ['电解镍']  # 先测试一个产品\n",
    "    \n",
    "    all_experiment_results = {}\n",
    "    \n",
    "    for config in test_configs:\n",
    "        print(f\"\\n{'#'*80}\")\n",
    "        print(f\"# 测试配置: {config.name}\")\n",
    "        print(f\"{'#'*80}\")\n",
    "        \n",
    "        config.print_summary()\n",
    "        \n",
    "        for product in products:\n",
    "            product_output_dir = os.path.join(base_dir, f\"实验_{config.name}\", f\"{product}预测结果\")\n",
    "            os.makedirs(product_output_dir, exist_ok=True)\n",
    "            \n",
    "            selected_features = load_selected_features(product, base_dir)\n",
    "            feature_file = os.path.join(base_dir, f\"{product}预测结果\", f\"{product}_特征工程数据.xlsx\")\n",
    "            \n",
    "            merged_df = pd.read_excel(feature_file)\n",
    "            merged_df['date'] = pd.to_datetime(merged_df['date'])\n",
    "            \n",
    "            results = aggregate_prediction_and_comparison(\n",
    "                merged_df, selected_features, product, product_output_dir, \n",
    "                config, use_search=False  # 批量测试时可能不想做超参数搜索\n",
    "            )\n",
    "            \n",
    "            if results:\n",
    "                key = f\"{product}_{config.name}\"\n",
    "                all_experiment_results[key] = results\n",
    "    \n",
    "    # 对比不同配置的结果\n",
    "    compare_configs(all_experiment_results)\n",
    "\n",
    "\n",
    "def compare_configs(all_experiment_results):\n",
    "    \"\"\"对比不同配置的结果\"\"\"\n",
    "    print(\"\\n\" + \"=\"*80)\n",
    "    print(\"不同配置的性能对比\")\n",
    "    print(\"=\"*80)\n",
    "    \n",
    "    comparison_data = []\n",
    "    \n",
    "    for key, results in all_experiment_results.items():\n",
    "        config = results['config']\n",
    "        model_performance = results['model_performance']\n",
    "        \n",
    "        for model_name, metrics in model_performance.items():\n",
    "            comparison_data.append({\n",
    "                '配置': config.display_name,\n",
    "                '配置名': config.name,\n",
    "                '模型': model_name,\n",
    "                'MAE': metrics.get('MAE', np.nan),\n",
    "                'WMAPE (%)': metrics.get('WMAPE (%)', np.nan),\n",
    "                'R²': metrics.get('R²', np.nan)\n",
    "            })\n",
    "    \n",
