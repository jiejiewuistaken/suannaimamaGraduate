    "\n",
    "# =============================================================================\n",
    "# 汇总报告生成函数\n",
    "# =============================================================================\n",
    "def generate_summary_report(all_results, base_dir, config):\n",
    "    \"\"\"生成聚合预测汇总报告 - 包含配置信息\"\"\"\n",
    "    print(f\"\\n生成{config.display_name}汇总报告...\")\n",
    "    \n",
    "    if not all_results:\n",
    "        print(\"没有结果可以汇总\")\n",
    "        return\n",
    "    \n",
    "    # 收集所有指标数据\n",
    "    summary_data = []\n",
    "    \n",
    "    for product, results in all_results.items():\n",
    "        model_performance = results.get('model_performance', {})\n",
    "        \n",
    "        for model_name, metrics in model_performance.items():\n",
    "            summary_data.append({\n",
    "                '产品': product,\n",
    "                '模型': model_name,\n",
    "                'MAE': metrics.get('MAE', np.nan),\n",
    "                'RMSE': metrics.get('RMSE', np.nan),\n",
    "                'R²': metrics.get('R²', np.nan),\n",
    "                # 'MAPE (%)': metrics.get('MAPE (%)', np.nan),\n",
    "                'WMAPE (%)': metrics.get('WMAPE (%)', np.nan)\n",
    "            })\n",
    "    \n",
    "    if summary_data:\n",
    "        summary_df = pd.DataFrame(summary_data)\n",
    "        \n",
    "        # 保存汇总数据 - 文件名包含配置\n",
    "        summary_path = os.path.join(base_dir, f\"{config.name}_预测汇总结果.xlsx\")\n",
