    "\n",
    "# =============================================================================\n",
    "# 时间窗口配置类 - 在这里统一管理所有窗口参数！！！\n",
    "# =============================================================================\n",
    "\n",
    "class WindowConfig:\n",
    "    \"\"\"时间窗口配置类\"\"\"\n",
    "    \n",
    "    def __init__(self, past_window=15, future_window=7, step=7):\n",
    "        self.past_window = past_window\n",
    "        self.future_window = future_window\n",
    "        self.step = step\n",
    "        \n",
    "        self.has_overlap = step < future_window\n",
    "        self.overlap_days = max(0, future_window - step)\n",
    "        self.overlap_ratio = self.overlap_days / future_window if future_window > 0 else 0\n",
    "        \n",
    "        self.name = f\"past{past_window}_future{future_window}_step{step}\"\n",
    "        self.display_name = f\"用过去{past_window}天预测未来{future_window}天(步长{step}天)\"\n",
    "        \n",
    "        self._validate()\n",
    "    \n",
    "    def _validate(self):\n",
    "        if self.past_window < 1:\n",
    "            raise ValueError(f\"past_window必须>=1，当前值: {self.past_window}\")\n",
    "        if self.future_window < 1:\n",
    "            raise ValueError(f\"future_window必须>=1，当前值: {self.future_window}\")\n",
    "        if self.step < 1:\n",
    "            raise ValueError(f\"step必须>=1，当前值: {self.step}\")\n",
    "        \n",
    "        if self.step > self.future_window:\n",
    "            print(f\"⚠️  警告: 步长({self.step})大于未来窗口({self.future_window})，预测目标之间会有间隔\")\n",
    "    \n",
    "    def print_summary(self):\n",
    "        print(\"\\n\" + \"=\"*80)\n",
    "        print(\"时间窗口配置\")\n",
    "        print(\"=\"*80)\n",
    "        print(f\"过去窗口 (特征): {self.past_window} 天\")\n",
    "        print(f\"未来窗口 (目标): {self.future_window} 天\")\n",
    "        print(f\"滑动步长:        {self.step} 天\")\n",
    "        print(f\"预测目标重叠:    {'是' if self.has_overlap else '否'}\")\n",
    "        if self.has_overlap:\n",
    "            print(f\"  - 重叠天数: {self.overlap_days} 天\")\n",
    "            print(f\"  - 重叠比例: {self.overlap_ratio*100:.1f}%\")\n",
    "        print(f\"配置名称:        {self.name}\")\n",
    "        print(f\"描述:            {self.display_name}\")\n",
    "        print(\"=\"*80 + \"\\n\")\n",
    "\n",
    "\n",
    "class PresetConfigs:\n",
    "    \"\"\"预定义配置方案\"\"\"\n",
    "    \n",
    "    @staticmethod\n",
    "    def get_config(preset_name=\"default\"):\n",
    "        configs = {\n",
    "            \"default\": WindowConfig(past_window=15, future_window=7, step=7),\n",
    "            \"weekly\": WindowConfig(past_window=7, future_window=7, step=7),\n",
    "            \"biweekly\": WindowConfig(past_window=14, future_window=14, step=14),\n",
    "            \"month\": WindowConfig(past_window=30, future_window=15, step=7),\n",
    "            \"overlap\": WindowConfig(past_window=15, future_window=15, step=7),\n",
    "        }\n",
    "        \n",
    "        if preset_name not in configs:\n",
    "            print(f\"警告: 未知的配置方案'{preset_name}'，使用默认配置\")\n",
    "            return configs[\"default\"]\n",
    "        \n",
    "        return configs[preset_name]\n",
    "    \n",
    "    @staticmethod\n",
    "    def list_presets():\n",
    "        print(\"\\n可用的预定义配置方案:\")\n",
    "        print(\"-\" * 80)\n",
    "        configs = {\n",
    "            \"default\": \"过去15天 → 未来7天，步长7天（无重叠）\",\n",
    "            \"weekly\": \"过去7天 → 未来7天，步长7天（无重叠）\",\n",
    "            \"biweekly\": \"过去14天 → 未来14天，步长14天（无重叠）\",\n",
    "            \"month\": \"过去30天 → 未来15天，步长7天（有重叠）\",\n",
    "            \"overlap\": \"过去15天 → 未来15天，步长7天（有重叠）\",\n",
    "        }\n",
    "        for name, desc in configs.items():\n",
    "            print(f\"  {name:12s} : {desc}\")\n",
    "        print(\"-\" * 80 + \"\\n\")\n",
    "\n",
    "\n",
    "# =============================================================================\n",
    "# 辅助函数（修改为接收config参数）\n",
    "# =============================================================================\n",
    "\n",
    "def calculate_adjusted_metrics_for_overlapping_windows(y_true, y_pred, config):\n",
    "    \"\"\"针对滑动窗口的重叠样本，计算调整后的评估指标\"\"\"\n",
    "    y_true, y_pred = np.array(y_true), np.array(y_pred)\n",
    "    n = len(y_true)\n",
    "    \n",
    "    mae = mean_absolute_error(y_true, y_pred)\n",
    "    rmse = np.sqrt(mean_squared_error(y_true, y_pred))\n",
    "    r2 = r2_score(y_true, y_pred)\n",
    "    \n",
    "    epsilon = 1e-10\n",
    "    # mape = np.mean(np.abs((y_true - y_pred) / (y_true + epsilon))) * 100\n",
    "    \n",
    "    weights = y_true / (y_true.sum() + epsilon)\n",
    "    weighted_ape = np.abs((y_true - y_pred) / (y_true + epsilon)) * weights\n",
    "    wmape = weighted_ape.sum() * 100\n",
    "    \n",
    "    # 使用config中的参数计算有效样本数\n",
    "    effective_n = n * (1 - config.overlap_ratio)\n",
    "    \n",
    "    residuals = y_true - y_pred\n",
    "    \n",
    "    if len(residuals) > 1:\n",
    "        autocorr_lag1 = np.corrcoef(residuals[:-1], residuals[1:])[0, 1]\n",
    "    else:\n",
    "        autocorr_lag1 = 0\n",
    "    \n",
    "    se_multiplier = np.sqrt(1 + 2 * abs(autocorr_lag1))\n",
    "    adjusted_mae_se = (rmse / np.sqrt(n)) * se_multiplier\n",
    "    \n",
    "    return {\n",
    "        'MAE': mae,\n",
    "        'RMSE': rmse,\n",
    "        'R²': r2,\n",
    "        # 'MAPE (%)': mape,\n",
    "        'WMAPE (%)': wmape,\n",
    "        'effective_sample_size': effective_n,\n",
    "        'autocorr_lag1': autocorr_lag1,\n",
    "        'adjusted_mae_se': adjusted_mae_se,\n",
    "        'mae_confidence_95': (mae - 1.96 * adjusted_mae_se, mae + 1.96 * adjusted_mae_se)\n",
    "    }\n",
    "\n",
    "def select_model_specific_features(features, X_train, y_train, model_type, model_name):\n",
    "    \"\"\"根据模型类型选择最适合的特征数量\"\"\"\n",
    "    from sklearn.feature_selection import SelectKBest, f_regression, RFE\n",
    "    from sklearn.feature_selection import mutual_info_regression\n",
    "    from sklearn.linear_model import LinearRegression\n",
    "    from sklearn.ensemble import RandomForestRegressor\n",
    "    \n",
    "    if model_type == 'linear':\n",
    "        k = min(12, max(8, len(features) // 3))\n",
    "        selector = SelectKBest(score_func=f_regression, k=k)\n",
    "        X_selected = selector.fit_transform(X_train, y_train)\n",
    "        selected_mask = selector.get_support()\n",
    "        selected_features = [features[i] for i in range(len(features)) if selected_mask[i]]\n",
    "        \n",
    "        if len(selected_features) > 10:\n",
    "            estimator = LinearRegression()\n",
    "            rfe = RFE(estimator, n_features_to_select=10)\n",
    "            rfe.fit(X_selected, y_train)\n",
    "            final_mask = rfe.get_support()\n",
    "            selected_features = [selected_features[i] for i in range(len(selected_features)) if final_mask[i]]\n",
    "        \n",
    "        return selected_features\n",
    "    \n",
    "    elif model_type == 'tree':\n",
    "        k = min(len(features), max(20, int(len(features) * 0.8)))\n",
    "        rf_temp = RandomForestRegressor(n_estimators=100, random_state=0)\n",
    "        rf_temp.fit(X_train, y_train)\n",
    "        \n",
    "        importance_scores = rf_temp.feature_importances_\n",
    "        feature_importance = list(zip(features, importance_scores))\n",
    "        feature_importance.sort(key=lambda x: x[1], reverse=True)\n",
    "        \n",
    "        selected_features = [feat[0] for feat in feature_importance[:k]]\n",
    "        return selected_features\n",
    "    \n",
    "    elif model_type in ['svm', 'neural']:\n",
    "        k = min(15, max(10, len(features) // 2))\n",
    "        selector = SelectKBest(score_func=mutual_info_regression, k=k)\n",
    "        selector.fit(X_train, y_train)\n",
    "        selected_mask = selector.get_support()\n",
    "        selected_features = [features[i] for i in range(len(features)) if selected_mask[i]]\n",
    "        return selected_features\n",
    "    \n",
    "    elif model_type == 'deep':  # ✅ 添加这个分支\n",
    "        k = min(10, max(6, len(features) // 4))\n",
    "        selector = SelectKBest(score_func=f_regression, k=k)\n",
    "        selector.fit(X_train, y_train)\n",
    "        selected_mask = selector.get_support()\n",
    "        selected_features = [features[i] for i in range(len(features)) if selected_mask[i]]\n",
    "        return selected_features\n",
    "    \n",
    "    else:\n",
    "        return features\n",
    "\n",
    "def get_preprocessed_data(X_train, X_test, y_train, model_type='tree'):\n",
    "    \"\"\"根据模型类型选择合适的预处理方法\"\"\"\n",
    "    \n",
    "    if model_type == 'tree':\n",
    "        scaler_X = RobustScaler()\n",
    "        scaler_y = None\n",
    "        \n",
    "        X_train_scaled = scaler_X.fit_transform(X_train)\n",
    "        X_test_scaled = scaler_X.transform(X_test)\n",
    "        y_train_scaled = y_train.copy()\n",
    "        \n",
    "    elif model_type == 'neural':\n",
    "        scaler_X = StandardScaler()\n",
    "        try:\n",
    "            scaler_y = PowerTransformer(method='yeo-johnson')\n",
    "            y_train_scaled = scaler_y.fit_transform(y_train.values.reshape(-1, 1)).flatten()\n",
    "        except:\n",
    "            scaler_y = StandardScaler()\n",
    "            y_train_scaled = scaler_y.fit_transform(y_train.values.reshape(-1, 1)).flatten()\n",
    "        \n",
    "        X_train_scaled = scaler_X.fit_transform(X_train)\n",
    "        X_test_scaled = scaler_X.transform(X_test)\n",
    "        \n",
    "    elif model_type == 'svm':\n",
    "        scaler_X = StandardScaler()\n",
    "        scaler_y = StandardScaler()\n",
    "        \n",
    "        X_train_scaled = scaler_X.fit_transform(X_train)\n",
    "        X_test_scaled = scaler_X.transform(X_test)\n",
    "        y_train_scaled = scaler_y.fit_transform(y_train.values.reshape(-1, 1)).flatten()\n",
    "    \n",
    "    elif model_type == 'linear':\n",
    "        scaler_X = StandardScaler()\n",
    "        scaler_y = None\n",
    "        \n",
    "        X_train_scaled = scaler_X.fit_transform(X_train)\n",
    "        X_test_scaled = scaler_X.transform(X_test)\n",
    "        y_train_scaled = y_train.copy()\n",
    "\n",
    "    elif model_type == 'deep':  # ✅ 添加这个分支\n",
    "        scaler_X = MinMaxScaler()\n",
    "        scaler_y = MinMaxScaler()\n",
    "        \n",
    "        X_train_scaled = scaler_X.fit_transform(X_train)\n",
    "        X_test_scaled = scaler_X.transform(X_test)\n",
    "        y_train_scaled = scaler_y.fit_transform(y_train.values.reshape(-1, 1)).flatten()\n",
    "    \n",
    "    return X_train_scaled, X_test_scaled, y_train_scaled, scaler_X, scaler_y\n",
    "\n",
    "def leak_free_fix_data_issues(train_df, test_df=None):\n",
    "    \"\"\"无数据泄漏的数据修复方法\"\"\"\n",
    "    print(\"执行无数据泄漏的数据清理...\")\n",
    "    \n",
    "    stats = {}\n",
    "    train_cleaned = train_df.copy()\n",
    "    \n",
    "    # 1. 处理负值需求\n",
    "    if 'demand' in train_cleaned.columns:\n",
    "        min_demand = train_cleaned['demand'].min()\n",
    "        if min_demand < 0:\n",
    "            print(f\"  修复负值需求: 最小值={min_demand}\")\n",
    "            train_cleaned['demand'] = train_cleaned['demand'].clip(lower=0)\n",
    "    \n",
    "    # 2. 处理无穷大值\n",
    "    numeric_cols = train_cleaned.select_dtypes(include=[np.number]).columns\n",
    "    for col in numeric_cols:\n",
    "        if col == 'date':\n",
    "            continue\n",
    "            \n",
    "        inf_count = np.isinf(train_cleaned[col]).sum()\n",
    "        if inf_count > 0:\n",
    "            finite_values = train_cleaned[col][np.isfinite(train_cleaned[col])]\n",
    "            if len(finite_values) > 0:\n",
    "                fill_value = finite_values.median()\n",
    "            else:\n",
    "                fill_value = 0\n",
    "            stats[f'{col}_inf_fill'] = fill_value\n",
    "            \n",
    "            train_cleaned[col] = train_cleaned[col].replace([np.inf, -np.inf], fill_value)\n",
    "    \n",
    "    # 3. 处理极值\n",
    "    for col in numeric_cols:\n",
    "        if col in ['date', 'demand']:\n",
    "            continue\n",
    "            \n",
    "        finite_values = train_cleaned[col][np.isfinite(train_cleaned[col])]\n",
    "        if len(finite_values) > 10:\n",
    "            Q1 = finite_values.quantile(0.25)\n",
    "            Q3 = finite_values.quantile(0.75)\n",
    "            IQR = Q3 - Q1\n",
    "            \n",
    "            if IQR > 0:\n",
    "                lower_bound = Q1 - 3 * IQR\n",
    "                upper_bound = Q3 + 3 * IQR\n",
    "                stats[f'{col}_lower_bound'] = lower_bound\n",
    "                stats[f'{col}_upper_bound'] = upper_bound\n",
    "                \n",
    "                extreme_mask = (train_cleaned[col] < lower_bound) | (train_cleaned[col] > upper_bound)\n",
    "                extreme_count = extreme_mask.sum()\n",
    "                if extreme_count > 0:\n",
    "                    train_cleaned.loc[extreme_mask, col] = np.clip(train_cleaned[col], lower_bound, upper_bound)\n",
    "        else:\n",
    "            stats[f'{col}_lower_bound'] = -1e10\n",
    "            stats[f'{col}_upper_bound'] = 1e10\n",
    "    \n",
    "    # 4. 处理NaN值\n",
    "    for col in train_cleaned.columns:\n",
    "        if train_cleaned[col].isna().any():\n",
    "            if col == 'date':\n",
    "                train_cleaned = train_cleaned.dropna(subset=['date'])\n",
    "            elif col == 'demand':\n",
    "                train_cleaned[col] = train_cleaned[col].fillna(method='ffill').fillna(0)\n",
    "            elif train_cleaned[col].dtype == 'object':\n",
    "                train_cleaned[col] = train_cleaned[col].fillna('unknown')\n",
    "            else:\n",
    "                fill_value = train_cleaned[col].median() if train_cleaned[col].notna().sum() > 0 else 0\n",
    "                stats[f'{col}_nan_fill'] = fill_value\n",
    "                train_cleaned[col] = train_cleaned[col].fillna(method='ffill').fillna(fill_value)\n",
    "    \n",
    "    # 5. 确保日期列有效\n",
    "    if 'date' in train_cleaned.columns:\n",
    "        train_cleaned['date'] = pd.to_datetime(train_cleaned['date'], errors='coerce')\n",
    "        train_cleaned = train_cleaned.dropna(subset=['date'])\n",
    "    \n",
    "    # 如果提供了测试集，使用训练集统计量处理测试集\n",
    "    if test_df is not None:\n",
    "        test_cleaned = test_df.copy()\n",
    "        \n",
    "        if 'demand' in test_cleaned.columns:\n",
    "            test_cleaned['demand'] = test_cleaned['demand'].clip(lower=0)\n",
    "        \n",
    "        for col in numeric_cols:\n",
    "            if col == 'date':\n",
    "                continue\n",
    "                \n",
    "            inf_count = np.isinf(test_cleaned[col]).sum()\n",
    "            if inf_count > 0:\n",
    "                fill_value = stats.get(f'{col}_inf_fill', 0)\n",
    "                test_cleaned[col] = test_cleaned[col].replace([np.inf, -np.inf], fill_value)\n",
    "        \n",
    "        for col in numeric_cols:\n",
    "            if col in ['date', 'demand']:\n",
    "                continue\n",
    "                \n",
    "            lower_bound = stats.get(f'{col}_lower_bound', -1e10)\n",
    "            upper_bound = stats.get(f'{col}_upper_bound', 1e10)\n",
    "            \n",
    "            extreme_mask = (test_cleaned[col] < lower_bound) | (test_cleaned[col] > upper_bound)\n",
    "            extreme_count = extreme_mask.sum()\n",
    "            if extreme_count > 0:\n",
    "                test_cleaned.loc[extreme_mask, col] = np.clip(test_cleaned[col], lower_bound, upper_bound)\n",
    "        \n",
    "        for col in test_cleaned.columns:\n",
    "            if test_cleaned[col].isna().any():\n",
    "                if col == 'date':\n",
    "                    test_cleaned = test_cleaned.dropna(subset=['date'])\n",
    "                elif col == 'demand':\n",
    "                    test_cleaned[col] = test_cleaned[col].fillna(method='ffill').fillna(0)\n",
    "                elif test_cleaned[col].dtype == 'object':\n",
    "                    test_cleaned[col] = test_cleaned[col].fillna('unknown')\n",
    "                else:\n",
    "                    fill_value = stats.get(f'{col}_nan_fill', 0)\n",
    "                    test_cleaned[col] = test_cleaned[col].fillna(method='ffill').fillna(fill_value)\n",
    "        \n",
    "        if 'date' in test_cleaned.columns:\n",
    "            test_cleaned['date'] = pd.to_datetime(test_cleaned['date'], errors='coerce')\n",
    "            test_cleaned = test_cleaned.dropna(subset=['date'])\n",
    "        \n",
    "        return train_cleaned, test_cleaned\n",
    "    \n",
    "    else:\n",
    "        return train_cleaned, stats\n",
    "\n",
    "def load_selected_features(product, output_dir):\n",
    "    \"\"\"加载已选择的最佳特征\"\"\"\n",
    "    product_output_dir = os.path.join(output_dir, f\"{product}预测结果\")\n",
    "    best_features_path = os.path.join(product_output_dir, f\"{product}_最佳特征.csv\")\n",
    "    \n",
    "    if os.path.exists(best_features_path):\n",
    "        best_features_df = pd.read_csv(best_features_path)\n",
    "        return best_features_df['feature'].tolist()\n",
    "    else:\n",
    "        print(f\"警告: 找不到{product}的最佳特征文件，使用默认特征\")\n",
    "        return [\n",
    "            'is_holiday', 'is_Monday', 'is_Tuesday', 'is_Wednesday', 'is_Thursday', \n",
    "            'is_Friday', 'is_Saturday', 'is_Sunday', 'is_weekend', 'demand_lag14', \n",
    "            'demand_ma3', 'demand_ma7', 'seasonal_factor', 'weekday_factor', \n",
    "            'is_future_volatile', 'trend', 'residual', 'seasonal'\n",
    "        ]\n",
    "\n",
    "# ========== 在这里添加新函数 ==========\n",
    "def calculate_baseline_ma(merged_df, test_cleaned, config):\n",
    "    \"\"\"动态计算适应不同窗口的移动平均基线\"\"\"\n",
    "    \n",
    "    future_window = config.future_window\n",
    "    \n",
    "    # 根据窗口大小选择聚合粒度和MA窗口\n",
    "    if future_window <= 7:\n",
    "        # 7天以内：使用周粒度\n",
    "        agg_demand = merged_df.resample('W-SUN', on='date')['demand'].sum().reset_index()\n",
    "        agg_demand.columns = ['period_start', 'agg_demand']\n",
    "        ma_window = 13  # 13周 ≈ 3个月\n",
    "        period_ratio = future_window / 7\n",
    "        baseline_name = \"13周移动平均\"\n",
    "    elif future_window <= 15:\n",
    "        # 8-15天：使用双周粒度\n",
    "        agg_demand = merged_df.resample('2W-SUN', on='date')['demand'].sum().reset_index()\n",
    "        agg_demand.columns = ['period_start', 'agg_demand']\n",
    "        ma_window = 6  # 6个双周 ≈ 3个月\n",
    "        period_ratio = future_window / 14\n",
    "        baseline_name = \"6期移动平均(双周)\"\n",
    "    else:\n",
    "        # 16天以上：使用月粒度\n",
    "        agg_demand = merged_df.resample('MS', on='date')['demand'].sum().reset_index()\n",
    "        agg_demand.columns = ['period_start', 'agg_demand']\n",
    "        ma_window = 3  # 3个月\n",
    "        period_ratio = future_window / 30\n",
    "        baseline_name = \"3期移动平均(月)\"\n",
    "    \n",
    "    # 计算移动平均\n",
    "    agg_demand['ma'] = agg_demand['agg_demand'].rolling(window=ma_window, min_periods=1).mean()\n",
    "    agg_demand['ma_forecast'] = agg_demand['ma'].shift(1)\n",
    "    \n",
    "    # 对每个测试周期预测\n",
    "    ma_predictions = []\n",
    "    \n",
    "    for i in range(len(test_cleaned)):\n",
    "        period_start = test_cleaned.iloc[i]['period_start']\n",
    "        period_end = test_cleaned.iloc[i]['period_end']\n",
    "        mid_date = period_start + (period_end - period_start) / 2\n",
    "        \n",
    "        # 找到最接近的历史周期\n",
    "        closest_periods = agg_demand[agg_demand['period_start'] <= mid_date]\n",
    "        if len(closest_periods) > 0:\n",
    "            closest_period = closest_periods.iloc[-1]\n",
    "        else:\n",
    "            closest_period = agg_demand.iloc[0]\n",
    "        \n",
    "        # 计算预测值（按比例缩放）\n",
    "        ma_pred = closest_period['ma_forecast'] * period_ratio\n",
    "        \n",
    "        # 处理NaN情况\n",
    "        if pd.isna(ma_pred):\n",
    "            if len(ma_predictions) > 0:\n",
    "                ma_pred = ma_predictions[-1]\n",
    "            else:\n",
    "                ma_pred = closest_period['agg_demand'] * period_ratio\n",
    "        \n",
    "        ma_predictions.append(ma_pred)\n",
    "    \n",
    "    print(f\"  使用 {baseline_name}（窗口={ma_window}期，缩放比例={period_ratio:.3f}）\")\n",
    "    \n",
    "    return np.array(ma_predictions), baseline_name\n",
    "\n",
    "# ===== 新增：多日聚合后的整数倍窗口评估工具 =====\n",
    "def compute_basic_metrics(y_true, y_pred):\n",
    "    y_true = np.array(y_true)\n",
    "    y_pred = np.array(y_pred)\n",
    "    mae = float(np.mean(np.abs(y_true - y_pred)))\n",
    "    rmse = float(np.sqrt(np.mean((y_true - y_pred) ** 2)))\n",
    "    ss_res = float(np.sum((y_true - y_pred) ** 2))\n",
    "    ss_tot = float(np.sum((y_true - np.mean(y_true)) ** 2))\n",
    "    r2 = float(1 - ss_res / ss_tot) if ss_tot > 0 else 0.0\n",
    "    wmape = float(100.0 * np.sum(np.abs(y_true - y_pred)) / (np.sum(np.abs(y_true)) + 1e-8))\n",
    "    return {'MAE': mae, 'RMSE': rmse, 'R²': r2, 'WMAPE (%)': wmape}\n",
    "\n",
    "def aggregate_block_metrics(y_true, model_predictions, period_infos, T_days, multiples=(2,)):\n",
    "    \"\"\"\n",
    "    将多日聚合预测(窗口= T_days)进一步按整数倍(如2x)聚合，并计算指标。\n",
    "    - 仅取与首个窗口对齐的非重叠窗口(起始日间隔 = T_days)以避免重复计数\n",
    "    - 每个块由连续的 m 个非重叠窗口组成，做总和对比\n",
    "    返回: (metrics_by_multiple, blocks_by_multiple)\n",
    "    \"\"\"\n",
    "    results = {}\n",
    "    blocks = {}\n",
    "    if not period_infos:\n",
    "        return results, blocks\n",
    "    anchor = pd.to_datetime(period_infos[0]['start'])\n",
    "    aligned_idx = [i for i, p in enumerate(period_infos) if ((pd.to_datetime(p['start']) - anchor).days % int(T_days) == 0)]\n",
    "    y_true = np.array(y_true)\n",
    "    for m in multiples:\n",
    "        m = int(m)\n",
    "        if m <= 0:\n",
    "            continue\n",
    "        num_blocks = len(aligned_idx) // m\n",
    "        if num_blocks <= 0:\n",
    "            continue\n",
    "        true_blocks = []\n",
    "        block_ranges = []\n",
    "        for b in range(num_blocks):\n",
    "            idxs = aligned_idx[b*m:(b+1)*m]\n",
    "            true_blocks.append(float(np.sum([y_true[i] for i in idxs])))\n",
    "            start_ts = pd.to_datetime(period_infos[idxs[0]]['start'])\n",
    "            end_ts = pd.to_datetime(period_infos[idxs[-1]]['end'])\n",
    "            block_ranges.append((start_ts, end_ts))\n",
    "        true_blocks = np.array(true_blocks)\n",
    "        pred_blocks_by_model = {}\n",
    "        for name, preds in model_predictions.items():\n",
    "            preds = np.array(preds)\n",
    "            pb = []\n",
    "            for b in range(num_blocks):\n",
    "                idxs = aligned_idx[b*m:(b+1)*m]\n",
    "                pb.append(float(np.sum(preds[idxs])))\n",
    "            pred_blocks_by_model[name] = np.array(pb)\n",
    "        metrics_by_model = {name: compute_basic_metrics(true_blocks, pb) for name, pb in pred_blocks_by_model.items()}\n",
    "        results[f'{m}x'] = metrics_by_model\n",
    "        blocks[f'{m}x'] = {\n",
    "            'block_ranges': block_ranges,\n",
    "            'y_true_blocks': true_blocks,\n",
    "            'pred_blocks_by_model': pred_blocks_by_model,\n",
    "        }\n",
    "    return results, blocks\n",
    "\n",
    "def aggregate_to_periods_sliding(daily_df, selected_features, config):\n",
    "    \"\"\"使用滑动窗口聚合数据 - 配置驱动版本\"\"\"\n",
    "    print(f\"\\n{config.display_name}\")\n",
    "    print(f\"聚合选定的{len(selected_features)}个特征\")\n",
    "    \n",
    "    daily_df['date'] = pd.to_datetime(daily_df['date'])\n",
    "    daily_df = daily_df.sort_values('date').reset_index(drop=True)\n",
    "    \n",
    "    aggregated_data = []\n",
    "    \n",
    "    past_window = config.past_window\n",
    "    future_window = config.future_window\n",
    "    step = config.step\n",
    "    \n",
    "    for i in range(past_window, len(daily_df) - future_window + 1, step):\n",
    "        past_data = daily_df.iloc[i-past_window:i]\n",
    "        future_data = daily_df.iloc[i:i+future_window]\n",
    "        \n",
    "        agg_row = {\n",
    "            'period_start': future_data['date'].min(),\n",
    "            'period_end': future_data['date'].max(),\n",
    "            'demand': future_data['demand'].sum(),\n",
    "        }\n",
    "        \n",
    "        for feature in selected_features:\n",
    "            if feature not in past_data.columns or feature == 'demand':\n",
    "                continue\n",
    "                \n",
    "            if past_data[feature].dtype in [np.float64, np.int64, np.float32, np.int32]:\n",
    "                agg_row[f'past_{feature}_mean'] = past_data[feature].mean()\n",
    "                agg_row[f'past_{feature}_last'] = past_data[feature].iloc[-1]\n",
    "                \n",
    "                # ========== 修改点1：删除这个波动性特征 ==========\n",
    "                # if past_data[feature].nunique() > 2:\n",
    "                #     agg_row[f'past_{feature}_std'] = past_data[feature].std()\n",
    "            else:\n",
    "                agg_row[f'past_{feature}_mode'] = past_data[feature].mode()[0] if len(past_data[feature].mode()) > 0 else past_data[feature].iloc[-1]\n",
    "        \n",
    "        # 未来特征保持不变\n",
    "        if 'is_holiday' in future_data.columns:\n",
    "            agg_row['future_holiday_days'] = future_data['is_holiday'].sum()\n",
    "            agg_row['future_holiday_ratio'] = future_data['is_holiday'].mean()\n",
    "        \n",
    "        if 'is_weekend' in future_data.columns:\n",
    "            agg_row['future_weekend_days'] = future_data['is_weekend'].sum()\n",
    "        \n",
    "        weekday_cols = ['is_Monday', 'is_Tuesday', 'is_Wednesday', 'is_Thursday', \n",
    "                       'is_Friday', 'is_Saturday', 'is_Sunday']\n",
    "        for col in weekday_cols:\n",
    "            if col in future_data.columns:\n",
    "                agg_row[f'future_{col}_count'] = future_data[col].sum()\n",
    "        \n",
    "        aggregated_data.append(agg_row)\n",
    "    \n",
    "    aggregated_df = pd.DataFrame(aggregated_data)\n",
    "    print(f\"聚合完成: {len(aggregated_df)}个窗口, {len(aggregated_df.columns)}个特征\")\n",
    "    \n",
    "    return aggregated_df\n",
    "\n",
    "def create_aggregated_row(past_data, future_data, selected_features):\n",
    "    \"\"\"创建单个聚合行\"\"\"\n",
    "    agg_row = {\n",
    "        'period_start': future_data['date'].min(),\n",
    "        'period_end': future_data['date'].max(),\n",
    "        'demand': future_data['demand'].sum(),\n",
    "    }\n",
    "    \n",
    "    for feature in selected_features:\n",
    "        if feature not in past_data.columns or feature == 'demand':\n",
    "            continue\n",
    "        \n",
    "        if past_data[feature].dtype in [np.float64, np.int64, np.float32, np.int32]:\n",
    "            agg_row[f'past_{feature}_mean'] = past_data[feature].mean()\n",
    "            agg_row[f'past_{feature}_last'] = past_data[feature].iloc[-1]\n",
    "            \n",
    "            if past_data[feature].nunique() > 2:\n",
    "                agg_row[f'past_{feature}_std'] = past_data[feature].std()\n",
    "        else:\n",
    "            mode_val = past_data[feature].mode()\n",
    "            agg_row[f'past_{feature}_mode'] = mode_val[0] if len(mode_val) > 0 else past_data[feature].iloc[-1]\n",
    "    \n",
    "    # 未来特征\n",
    "    if 'is_holiday' in future_data.columns:\n",
    "        agg_row['future_holiday_days'] = future_data['is_holiday'].sum()\n",
    "        agg_row['future_holiday_ratio'] = future_data['is_holiday'].mean()\n",
    "    \n",
    "    if 'is_weekend' in future_data.columns:\n",
    "        agg_row['future_weekend_days'] = future_data['is_weekend'].sum()\n",
    "    \n",
    "    weekday_cols = ['is_Monday', 'is_Tuesday', 'is_Wednesday', 'is_Thursday', \n",
    "                   'is_Friday', 'is_Saturday', 'is_Sunday']\n",
    "    for col in weekday_cols:\n",
    "        if col in future_data.columns:\n",
    "            agg_row[f'future_{col}_count'] = future_data[col].sum()\n",
    "    \n",
    "    return agg_row\n",
    "\n",
