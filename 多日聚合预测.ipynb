    "\n",
    "def recursive_predict_future(predictor, last_known_data, future_days, config, daily_features):\n",
    "    \"\"\"\n",
    "    递归预测未来需求，使用预测值而不是真实值构建特征\n",
    "    \n",
    "    参数:\n",
    "    - predictor: 训练好的预测器\n",
    "    - last_known_data: 最后已知的历史数据（DataFrame）\n",
    "    - future_days: 需要预测的天数\n",
    "    - config: 窗口配置\n",
    "    - daily_features: 需要用到的特征列表\n",
    "    \"\"\"\n",
    "    import pandas as pd\n",
    "    import numpy as np\n",
    "    \n",
    "    # 复制历史数据作为基础\n",
    "    predicted_data = last_known_data.copy()\n",
    "    \n",
    "    # 逐天预测\n",
    "    for day_idx in range(future_days):\n",
    "        # 获取过去窗口的数据用于构建特征\n",
    "        if len(predicted_data) >= config.past_window:\n",
    "            past_data = predicted_data.iloc[-config.past_window:]\n",
    "        else:\n",
    "            # 如果数据不足，用现有的所有数据\n",
    "            past_data = predicted_data.copy()\n",
    "        \n",
    "        # 构建当天的特征\n",
    "        current_features = {}\n",
    "        \n",
    "        for feature in daily_features:\n",
    "            if feature not in past_data.columns or feature == 'demand':\n",
    "                continue\n",
    "            \n",
    "            if past_data[feature].dtype in [np.float64, np.int64, np.float32, np.int32]:\n",
    "                current_features[f'past_{feature}_mean'] = past_data[feature].mean()\n",
    "                current_features[f'past_{feature}_last'] = past_data[feature].iloc[-1]\n",
    "                \n",
    "                if past_data[feature].nunique() > 2:\n",
    "                    current_features[f'past_{feature}_std'] = past_data[feature].std()\n",
    "            else:\n",
    "                mode_val = past_data[feature].mode()\n",
    "                current_features[f'past_{feature}_mode'] = mode_val[0] if len(mode_val) > 0 else past_data[feature].iloc[-1]\n",
    "        \n",
    "        # 添加未来特征（如果适用）\n",
    "        next_date = predicted_data.iloc[-1]['date'] + pd.Timedelta(days=1)\n",
    "        \n",
    "        # 这里需要根据日期计算未来特征\n",
    "        # 例如: is_holiday, is_weekend等\n",
    "        future_row = {\n",
    "            'date': next_date,\n",
    "            'is_holiday': 0,  # 需要根据实际情况计算\n",
    "            'is_weekend': 1 if next_date.dayofweek >= 5 else 0,\n",
    "        }\n",
    "        \n",
    "        # 使用预测器预测当天需求\n",
    "        try:\n",
    "            pred_demand = predictor.predict_period_aggregate(current_features)\n",
    "        except:\n",
    "            pred_demand = predicted_data['demand'].iloc[-1]  # fallback\n",
    "        \n",
    "        future_row['demand'] = pred_demand\n",
    "        \n",
    "        # 添加到预测数据中\n",
    "        predicted_data = pd.concat([predicted_data, pd.DataFrame([future_row])], ignore_index=True)\n",
    "    \n",
    "    return predicted_data\n",
    "\n",
    "# =============================================================================\n",
    "# 主要预测函数（修改为接收config）\n",
    "# =============================================================================\n",
    "\n",
    "def calculate_model_ranking(model_performance, exclude_baselines=True):\n",
    "    \"\"\"计算模型综合排名\"\"\"\n",
    "    \n",
    "    # 排除基线模型\n",
    "    baseline_models = ['LinearRegression', '13周移动平均'] if exclude_baselines else []\n",
    "    filtered_performance = {k: v for k, v in model_performance.items() if k not in baseline_models}\n",
    "    \n",
    "    if not filtered_performance:\n",
    "        return []\n",
    "    \n",
    "    # 主要指标权重\n",
    "    weights = {\n",
    "        'WMAPE (%)': 0.4,\n",
    "        'MAE': 0.3,\n",
    "        'R²': 0.3\n",
    "    }\n",
    "    \n",
    "    # 计算综合排名\n",
    "    models = list(filtered_performance.keys())\n",
    "    rankings = {}\n",
    "    \n",
    "    for metric, weight in weights.items():\n",
    "        if metric == 'R²':\n",
    "            sorted_models = sorted(models, \n",
    "                                 key=lambda x: filtered_performance[x].get(metric, -1), \n",
    "                                 reverse=True)\n",
    "        else:\n",
    "            sorted_models = sorted(models, \n",
    "                                 key=lambda x: filtered_performance[x].get(metric, float('inf')))\n",
    "        \n",
    "        for i, model in enumerate(sorted_models):\n",
    "            if model not in rankings:\n",
    "                rankings[model] = 0\n",
    "            rankings[model] += weight * (len(models) - i)\n",
    "    \n",
    "    # 按综合得分排序\n",
    "    final_ranking = sorted(rankings.items(), key=lambda x: x[1], reverse=True)\n",
    "    \n",
    "    print(\"\\n基于WMAPE(40%)+MAE(30%)+R²(30%)的综合排名:\")\n",
    "    for i, (model, score) in enumerate(final_ranking, 1):\n",
    "        wmape = filtered_performance[model].get('WMAPE (%)', 'N/A')\n",
    "        mae = filtered_performance[model].get('MAE', 'N/A')\n",
    "        r2 = filtered_performance[model].get('R²', 'N/A')\n",
    "        if isinstance(wmape, (int, float)):\n",
    "            wmape = f\"{wmape:.2f}%\"\n",
    "        if isinstance(mae, (int, float)):\n",
    "            mae = f\"{mae:.0f}\"\n",
    "        if isinstance(r2, (int, float)):\n",
    "            r2 = f\"{r2:.4f}\"\n",
    "        print(f\"  {i}. {model}: {score:.2f}分 (WMAPE: {wmape}, MAE: {mae}, R²: {r2})\")\n",
    "    \n",
    "    return final_ranking\n",
    "\n",
    "def create_performance_based_ensemble(model_predictions, model_performance, exclude_baselines=True):\n",
    "    \"\"\"基于性能创建集成预测，排除基线模型\"\"\"\n",
    "    \n",
    "    # 排除基线模型\n",
    "    baseline_models = ['LinearRegression', '13周移动平均'] if exclude_baselines else []\n",
    "    \n",
    "    # 过滤模型：排除基线模型和表现极差的模型\n",
    "    filtered_predictions = {}\n",
    "    valid_performances = {}\n",
    "    \n",
    "    for name, pred in model_predictions.items():\n",
    "        if name not in baseline_models and name in model_performance:\n",
    "            # 排除WMAPE > 80%的模型（表现极差）\n",
    "            wmape = model_performance[name].get('WMAPE (%)', float('inf'))\n",
    "            if wmape < 80:\n",
    "                filtered_predictions[name] = pred\n",
    "                valid_performances[name] = model_performance[name]\n",
    "    \n",
    "    if len(filtered_predictions) < 2:\n",
    "        print(\"可用于集成的模型数量不足，使用所有非基线模型\")\n",
    "        for name, pred in model_predictions.items():\n",
    "            if name not in baseline_models and name in model_performance:\n",
    "                filtered_predictions[name] = pred\n",
    "                valid_performances[name] = model_performance[name]\n",
    "    \n",
    "    if len(filtered_predictions) < 2:\n",
    "        return None, None\n",
    "    \n",
    "    # 基于多指标计算权重\n",
    "    def calculate_composite_score(metrics):\n",
    "        \"\"\"计算综合得分\"\"\"\n",
    "        wmape = metrics.get('WMAPE (%)', 100)\n",
    "        mae = metrics.get('MAE', float('inf'))\n",
    "        r2 = max(0, metrics.get('R²', 0))\n",
    "        \n",
    "        # 归一化分数\n",
    "        wmape_score = 1.0 / (1.0 + wmape / 50)\n",
    "        mae_score = 1.0 / (1.0 + mae / 50000)\n",
    "        r2_score = r2\n",
    "        \n",
    "        # 加权组合：WMAPE(40%) + MAE(30%) + R²(30%)\n",
    "        composite = 0.4 * wmape_score + 0.3 * mae_score + 0.3 * r2_score\n",
    "        return composite\n",
    "    \n",
    "    # 计算权重\n",
    "    scores = {name: calculate_composite_score(metrics) \n",
    "              for name, metrics in valid_performances.items()}\n",
    "    \n",
    "    total_score = sum(scores.values())\n",
    "    if total_score == 0:\n",
    "        weights = {name: 1.0/len(scores) for name in scores.keys()}\n",
    "    else:\n",
    "        weights = {name: score / total_score for name, score in scores.items()}\n",
    "    \n",
    "    # 计算集成预测\n",
    "    pred_lengths = [len(pred) for pred in filtered_predictions.values()]\n",
    "    base_length = min(pred_lengths)\n",
    "    \n",
    "    ensemble_pred = np.zeros(base_length)\n",
    "    \n",
    "    print(\"基于性能的集成权重:\")\n",
    "    for model_name, pred in filtered_predictions.items():\n",
    "        weight = weights[model_name]\n",
    "        wmape = valid_performances[model_name]['WMAPE (%)']\n",
    "        print(f\"  {model_name}: {weight:.3f} (WMAPE: {wmape:.2f}%)\")\n",
    "        \n",
    "        pred_adj = pred[:base_length]\n",
    "        ensemble_pred += weight * pred_adj\n",
    "    \n",
    "    return np.maximum(ensemble_pred, 0), weights\n",
    "\n",
    "def train_and_apply_stacking_blocks(train_periods, y_train, trained_models, model_predictions,\n",
    "                                    period_infos, config, split_ratio=0.8):\n",
    "    \"\"\"Stacking（块级）：在训练期的后段（按照窗口）上训练二层回归器，\n",
    "    使用各基模型的窗口预测作为元特征，输出对测试窗口的Stacking预测。\n",
    "    \"\"\"\n",
    "    try:\n",
    "        from sklearn.linear_model import LassoCV\n",
    "        from sklearn.base import clone\n",
    "        import numpy as np\n",
    "\n",
    "        base_names = [n for n, m in trained_models.items() if m.get('type') != 'deep' and n not in ['LinearRegression']]\n",
    "        if len(base_names) < 2:\n",
    "            return None, None\n",
    "\n",
    "        split_idx = int(len(train_periods) * split_ratio)\n",
    "        if split_idx <= 0 or split_idx >= len(train_periods):\n",
    "            return None, None\n",
    "\n",
    "        X_tr = train_periods.iloc[:split_idx]\n",
    "        y_tr = y_train.iloc[:split_idx]\n",
    "        X_val = train_periods.iloc[split_idx:]\n",
    "        y_val = y_train.iloc[split_idx:]\n",
    "\n",
    "        val_meta_cols = []\n",
    "        for name in base_names:\n",
    "            info = trained_models[name]\n",
    "            feats = [f for f in info['features'] if f in train_periods.columns]\n",
    "            if len(feats) == 0:\n",
    "                continue\n",
    "            model = info['model']\n",
    "            model_type = info['type']\n",
    "            model_clone = clone(model)\n",
    "            X_tr_proc, X_val_proc, y_tr_proc, scaler_X, scaler_y = get_preprocessed_data(X_tr[feats], X_val[feats], y_tr, model_type)\n",
    "            model_clone.fit(X_tr_proc, y_tr_proc)\n",
    "            p_val = model_clone.predict(X_val_proc)\n",
    "            if scaler_y is not None:\n",
    "                p_val = scaler_y.inverse_transform(p_val.reshape(-1,1)).flatten()\n",
    "            val_meta_cols.append(p_val)\n",
    "\n",
    "        if len(val_meta_cols) < 2:\n",
    "            return None, None\n",
    "        X_meta_val = np.vstack(val_meta_cols).T\n",
    "        meta = LassoCV(alphas=None, cv=min(3, max(2, len(X_meta_val)//5)))\n",
    "        meta.fit(X_meta_val, y_val.values)\n",
    "\n",
    "        base_len = min(len(model_predictions[n]) for n in base_names if n in model_predictions)\n",
    "        X_meta_test = np.vstack([np.array(model_predictions[n][:base_len]) for n in base_names]).T\n",
    "        stacking_pred = meta.predict(X_meta_test)\n",
    "        return stacking_pred, dict(zip(base_names, getattr(meta, 'coef_', [])))\n",
    "    except Exception:\n",
    "        return None, None\n",
    "\n",
    "def aggregate_prediction_recursive(\n",
    "    merged_df, selected_features, product_name, output_dir, config, \n",
    "    trained_predictors, test_start_date='2024-01-01'):\n",
    "    \"\"\"\n",
    "    窗口级别递归预测：使用预测窗口值构建下一窗口特征\n",
    "    \"\"\"\n",
    "    print(f\"\\n{'='*80}\")\n",
    "    print(f\"窗口级递归预测（测试集从{test_start_date}开始使用预测值）\")\n",
    "    print(f\"{'='*80}\")\n",
    "    \n",
    "    test_start_date = pd.to_datetime(test_start_date)\n",
    "    train_df = merged_df[merged_df['date'] < test_start_date].copy()\n",
    "    test_df = merged_df[merged_df['date'] >= test_start_date].copy()\n",
    "    \n",
    "    past_window = config.past_window\n",
    "    future_window = config.future_window\n",
    "    step = config.step\n",
    "    \n",
    "    # ========== 初始化：用训练集最后几个真实窗口 ==========\n",
    "    window_history = []\n",
    "    \n",
    "    # 获取训练集最后N个窗口作为初始历史\n",
    "    num_init_windows = max(3, past_window // future_window)\n",
    "    start_idx = max(0, len(train_df) - num_init_windows * future_window)\n",
    "    \n",
    "    for i in range(start_idx, len(train_df) - future_window + 1, step):\n",
    "        if i + future_window > len(train_df):\n",
    "            break\n",
    "        window_data = train_df.iloc[i:i+future_window]\n",
    "        \n",
    "        # 构建与训练时相同的特征\n",
    "        past_data = train_df.iloc[max(0, i-past_window):i]\n",
    "        \n",
    "        window_features = {\n",
    "            'period_start': window_data['date'].min(),\n",
    "            'period_end': window_data['date'].max(),\n",
    "            'demand': window_data['demand'].sum(),\n",
    "        }\n",
    "        \n",
    "        # 复制你原来的特征逻辑\n",
    "        for feature in selected_features:\n",
    "            if feature not in past_data.columns or feature == 'demand':\n",
    "                continue\n",
    "            \n",
    "            if past_data[feature].dtype in [np.float64, np.int64, np.float32, np.int32]:\n",
    "                window_features[f'past_{feature}_mean'] = past_data[feature].mean()\n",
    "                window_features[f'past_{feature}_last'] = past_data[feature].iloc[-1]\n",
    "            else:\n",
    "                mode_val = past_data[feature].mode()\n",
    "                window_features[f'past_{feature}_mode'] = mode_val[0] if len(mode_val) > 0 else past_data[feature].iloc[-1]\n",
    "        \n",
    "        # 未来特征\n",
    "        if 'is_holiday' in window_data.columns:\n",
    "            window_features['future_holiday_days'] = window_data['is_holiday'].sum()\n",
    "            window_features['future_holiday_ratio'] = window_data['is_holiday'].mean()\n",
    "        \n",
    "        if 'is_weekend' in window_data.columns:\n",
    "            window_features['future_weekend_days'] = window_data['is_weekend'].sum()\n",
    "        \n",
    "        weekday_cols = ['is_Monday', 'is_Tuesday', 'is_Wednesday', 'is_Thursday', \n",
    "                       'is_Friday', 'is_Saturday', 'is_Sunday']\n",
    "        for col in weekday_cols:\n",
    "            if col in window_data.columns:\n",
    "                window_features[f'future_{col}_count'] = window_data[col].sum()\n",
    "        \n",
    "        window_history.append(window_features)\n",
    "    \n",
    "    print(f\"初始化窗口历史: {len(window_history)}个真实窗口\")\n",
    "    \n",
    "    # ========== 递归预测测试集 ==========\n",
    "    all_predictions = {name: [] for name in trained_predictors.keys()}\n",
    "    actual_demands = []\n",
    "    period_infos = []\n",
    "    \n",
    "    current_date = test_start_date\n",
    "    prediction_count = 0\n",
    "    \n",
    "    while current_date <= test_df['date'].max():\n",
    "        prediction_count += 1\n",
    "        period_start = current_date\n",
    "        period_end = min(current_date + pd.Timedelta(days=future_window - 1), \n",
    "                        test_df['date'].max())\n",
    "        \n",
    "        print(f\"\\n预测窗口 {prediction_count}: {period_start.date()} ~ {period_end.date()}\")\n",
    "        \n",
    "        # 获取真实需求（仅用于评估）\n",
    "        actual_window = test_df[(test_df['date'] >= period_start) & \n",
    "                               (test_df['date'] <= period_end)]\n",
    "        actual_demand = actual_window['demand'].sum() if len(actual_window) > 0 else 0\n",
    "        \n",
    "        # ========== 构建特征：基于窗口历史（最近N个窗口的demand是预测值）==========\n",
    "        # 用最近的past_window天对应的窗口来构建\"过去特征\"\n",
    "        # 简化：用最近的窗口作为\"过去数据\"\n",
    "        if len(window_history) > 0:\n",
    "            recent_window = window_history[-1]\n",
    "            current_features = {}\n",
    "            \n",
    "            # 复制过去特征（这些是基于预测值计算的）\n",
    "            for key, value in recent_window.items():\n",
    "                if key.startswith('past_') or key.startswith('future_'):\n",
    "                    current_features[key] = value\n",
    "        else:\n",
    "            current_features = {}\n",
    "        \n",
    "        # 更新未来特征（基于当前窗口的已知日期信息）\n",
    "        if len(actual_window) > 0:\n",
    "            if 'is_holiday' in actual_window.columns:\n",
    "                current_features['future_holiday_days'] = actual_window['is_holiday'].sum()\n",
    "                current_features['future_holiday_ratio'] = actual_window['is_holiday'].mean()\n",
    "            \n",
    "            if 'is_weekend' in actual_window.columns:\n",
    "                current_features['future_weekend_days'] = actual_window['is_weekend'].sum()\n",
    "            \n",
    "            weekday_cols = ['is_Monday', 'is_Tuesday', 'is_Wednesday', 'is_Thursday', \n",
    "                           'is_Friday', 'is_Saturday', 'is_Sunday']\n",
    "            for col in weekday_cols:\n",
    "                if col in actual_window.columns:\n",
    "                    current_features[f'future_{col}_count'] = actual_window[col].sum()\n",
    "        \n",
    "        # ========== 预测 ==========\n",
    "        for model_name, predictor in trained_predictors.items():\n",
    "            try:\n",
    "                pred = predictor.predict_period_aggregate(current_features)\n",
    "                all_predictions[model_name].append(pred)\n",
    "                print(f\"  {model_name}: {pred:.0f}\")\n",
    "            except Exception as e:\n",
    "                print(f\"  {model_name} 预测失败: {str(e)}\")\n",
    "                all_predictions[model_name].append(0)\n",
    "        \n",
    "        # ========== 将预测值加入历史 ==========\n",
    "        if 'XGBoost' in all_predictions and len(all_predictions['XGBoost']) > 0:\n",
    "            pred_demand = all_predictions['XGBoost'][-1]\n",
    "        else:\n",
    "            pred_demand = np.mean([v[-1] for v in all_predictions.values() if len(v) > 0])\n",
    "        \n",
    "        # 构建新窗口特征（用预测的demand）\n",
    "        new_window = {\n",
    "            'period_start': period_start,\n",
    "            'period_end': period_end,\n",
    "            'demand': pred_demand,  # ⚠️ 预测值\n",
    "        }\n",
    "        \n",
    "        # 复制当前特征作为下一窗口的\"过去特征\"\n",
    "        for key, value in current_features.items():\n",
    "            new_window[key] = value\n",
    "        \n",
    "        window_history.append(new_window)\n",
    "        \n",
    "        actual_demands.append(actual_demand)\n",
    "        period_infos.append({'start': period_start, 'end': period_end})\n",
    "        \n",
    "        print(f\"  实际需求: {actual_demand:.0f}\")\n",
    "        \n",
    "        # 移动到下一窗口\n",
    "        current_date = current_date + pd.Timedelta(days=step)\n",
    "    \n",
    "    return {\n",
    "        'predictions': all_predictions,\n",
    "        'actuals': actual_demands,\n",
    "        'period_infos': period_infos,\n",
    "    }\n",
    "\n",
    "def aggregate_prediction_and_comparison(merged_df, selected_features, product_name, output_dir, config, use_search=True):\n",
    "    \"\"\"执行聚合预测并与基线比较\"\"\"\n",
    "    print(f\"\\n{'='*80}\")\n",
    "    print(f\"开始为{product_name}执行{config.display_name}预测比较\")\n",
    "    print(f\"{'='*80}\")\n",
    "    \n",
    "    merged_df['date'] = pd.to_datetime(merged_df['date'])\n",
    "    merged_df = merged_df.sort_values('date').reset_index(drop=True)\n",
    "    \n",
    "    # 聚合数据 - 传入config\n",
    "    aggregated_df = aggregate_to_periods_sliding(merged_df, selected_features, config)\n",
    "    \n",
    "    total_periods = len(aggregated_df)\n",
    "    train_size = int(total_periods * 0.8)\n",
    "    \n",
    "    train = aggregated_df.iloc[:train_size]\n",
    "    test = aggregated_df.iloc[train_size:]\n",
    "    \n",
    "    print(f\"总周期数: {total_periods}, 训练: {len(train)}, 测试: {len(test)}\")\n",
    "    \n",
    "    if test.empty:\n",
    "        print(f\"警告: {product_name}没有测试数据，无法进行预测比较\")\n",
    "        return None\n",
    "    \n",
    "    # 数据清理\n",
    "    train_cleaned, test_cleaned = leak_free_fix_data_issues(train, test)\n",
