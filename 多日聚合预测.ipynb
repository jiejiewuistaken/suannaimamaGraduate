    "\n",
    "class AggregatePredictor:\n",
    "    \"\"\"通用聚合预测器类\"\"\"\n",
    "    \n",
    "    def __init__(self, model, scaler_X, scaler_y, features, model_type, model_name, config):\n",
    "        self.model = model\n",
    "        self.scaler_X = scaler_X\n",
    "        self.scaler_y = scaler_y\n",
    "        self.features = features\n",
    "        self.model_type = model_type\n",
    "        self.model_name = model_name\n",
    "        self.config = config\n",
    "        self.prediction_history = []\n",
    "    \n",
    "    def predict_period_aggregate(self, period_features_dict):\n",
    "        \"\"\"预测聚合需求\"\"\"\n",
    "        try:\n",
    "            feature_vector = []\n",
    "            for feature in self.features:\n",
    "                value = period_features_dict.get(feature, 0)\n",
    "                if pd.isna(value) or np.isinf(value):\n",
    "                    value = 0\n",
    "                feature_vector.append(value)\n",
    "            \n",
    "            feature_vector = np.array(feature_vector).reshape(1, -1)\n",
    "            feature_scaled = self.scaler_X.transform(feature_vector)\n",
    "            pred_scaled = self.model.predict(feature_scaled)[0]\n",
    "            \n",
    "            if self.scaler_y is not None:\n",
    "                pred = self.scaler_y.inverse_transform([[pred_scaled]])[0, 0]\n",
    "            else:\n",
    "                pred = pred_scaled\n",
    "            \n",
    "            pred = max(0, pred)\n",
    "            self.prediction_history.append(pred)\n",
    "            \n",
    "            return pred\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"  {self.model_name} 聚合预测失败: {str(e)}\")\n",
    "            return 0\n",
    "    \n",
    "    def predict_test_periods(self, test_periods_data):\n",
    "        \"\"\"预测所有周期\"\"\"\n",
    "        predictions = []\n",
    "        \n",
    "        for i in range(len(test_periods_data)):\n",
    "            period_features = test_periods_data.iloc[i][self.features].to_dict()\n",
    "            pred = self.predict_period_aggregate(period_features)\n",
    "            predictions.append(pred)\n",
    "        \n",
    "        return np.array(predictions)\n",
    "\n",
    "class SequenceAggregatePredictor:\n",
    "    \"\"\"序列聚合预测器类 - 用于LSTM等模型的聚合预测\"\"\"\n",
    "    \n",
    "    def __init__(self, model, scaler_X, scaler_y, features, sequence_length, model_name, config):\n",
    "        self.model = model\n",
    "        self.scaler_X = scaler_X\n",
    "        self.scaler_y = scaler_y\n",
    "        self.features = features\n",
    "        self.sequence_length = sequence_length\n",
    "        self.model_name = model_name\n",
    "        self.config = config  # ✅ 添加config\n",
    "        self.feature_history = []\n",
    "    \n",
    "    def add_period_features(self, period_features_dict):\n",
    "        \"\"\"添加该周期特征到历史记录\"\"\"\n",
    "        feature_vector = []\n",
    "        for feature in self.features:\n",
    "            value = period_features_dict.get(feature, 0)\n",
    "            if pd.isna(value) or np.isinf(value):\n",
    "                value = 0\n",
    "            feature_vector.append(value)\n",
    "        \n",
    "        feature_scaled = self.scaler_X.transform([feature_vector])[0]\n",
    "        self.feature_history.append(feature_scaled)\n",
    "        \n",
    "        if len(self.feature_history) > self.sequence_length:\n",
    "            self.feature_history.pop(0)\n",
    "    \n",
    "    def predict_single_period(self):\n",
    "        \"\"\"基于历史序列预测当前周期需求\"\"\"\n",
    "        try:\n",
    "            if len(self.feature_history) < self.sequence_length:\n",
    "                return 0\n",
    "            \n",
    "            sequence = np.array(self.feature_history[-self.sequence_length:]).reshape(1, self.sequence_length, -1)\n",
    "            pred_scaled = self.model.predict(sequence, verbose=0)[0, 0]\n",
    "            \n",
    "            if self.scaler_y is not None:\n",
    "                pred = self.scaler_y.inverse_transform([[pred_scaled]])[0, 0]\n",
    "            else:\n",
    "                pred = pred_scaled\n",
    "            \n",
    "            return max(0, pred)\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"  {self.model_name} 序列聚合预测失败: {str(e)}\")\n",
    "            return 0\n",
    "    \n",
    "    def predict_test_periods(self, test_periods_data, train_periods_data=None):\n",
    "        \"\"\"预测所有测试周期\"\"\"\n",
    "        predictions = []\n",
    "        \n",
    "        # 用训练数据的最后几个周期初始化历史\n",
    "        if train_periods_data is not None and len(train_periods_data) >= self.sequence_length:\n",
    "            for i in range(-self.sequence_length, 0):\n",
    "                period_features = train_periods_data.iloc[i][self.features].to_dict()\n",
    "                self.add_period_features(period_features)\n",
    "        \n",
    "        # 逐周期预测测试数据\n",
    "        for i in range(len(test_periods_data)):\n",
    "            pred = self.predict_single_period()\n",
    "            predictions.append(pred)\n",
    "            \n",
    "            period_features = test_periods_data.iloc[i][self.features].to_dict()\n",
    "            self.add_period_features(period_features)\n",
    "        \n",
    "        return np.array(predictions)\n",
    "\n",
    "\n",
    "def train_models_with_improved_defaults(train_periods, working_features, y_train, product_name, config):\n",
    "    \"\"\"使用改进默认参数的快速训练 - 配置驱动版本\"\"\"\n",
    "    \n",
    "    models_config = create_improved_models()\n",
    "    \n",
    "    trained_models = {}\n",
    "    best_params_dict = {}\n",
    "    \n",
    "    for name, model_config in models_config.items():\n",
    "        print(f\"\\n训练 {name} 模型（改进默认参数）...\")\n",
    "        \n",
    "        try:\n",
    "            model = model_config['model']\n",
    "            model_type = model_config['type']\n",
    "            \n",
    "            model_features = select_model_specific_features(\n",
    "                working_features, \n",
    "                train_periods[working_features], \n",
    "                y_train, \n",
    "                model_type, \n",
    "                name\n",
    "            )\n",
    "            \n",
    "            X_train = train_periods[model_features]\n",
    "            X_train_processed, _, y_train_processed, scaler_X, scaler_y = get_preprocessed_data(\n",
    "                X_train, X_train, y_train, model_type\n",
    "            )\n",
    "            \n",
    "            # 对SVR做网格搜索\n",
    "            if name == 'SVR':\n",
    "                print(f\"  执行 {name} 网格搜索...\")\n",
    "                \n",
    "                param_grid = {\n",
    "                    'C': [100, 1000, 5000],\n",
    "                    'gamma': ['scale', 'auto', 0.01],\n",
    "                    'epsilon': [0.01, 0.05, 0.1]\n",
    "                }\n",
    "                \n",
    "                tscv = TimeSeriesSplit(n_splits=min(3, len(X_train_processed) // 3))\n",
    "                grid_search = GridSearchCV(\n",
    "                    model, param_grid, \n",
    "                    scoring='neg_mean_absolute_error',\n",
    "                    cv=tscv, n_jobs=1, verbose=0\n",
    "                )\n",
    "                grid_search.fit(X_train_processed, y_train_processed)\n",
    "                model = grid_search.best_estimator_\n",
    "                \n",
    "                print(f\"  最佳参数: {grid_search.best_params_}\")\n",
    "                print(f\"  最佳得分: {grid_search.best_score_:.2f}\")\n",
    "            else:\n",
    "                # 其他模型正常训练\n",
    "                if name == 'XGBoost':\n",
    "                    eval_set = [(X_train_processed, y_train_processed)]\n",
    "                    model.fit(X_train_processed, y_train_processed, \n",
    "                            eval_set=eval_set, verbose=False)\n",
    "                else:\n",
    "                    model.fit(X_train_processed, y_train_processed)\n",
    "            \n",
    "            trained_models[name] = {\n",
    "                'model': model,\n",
    "                'features': model_features,\n",
    "                'scaler_X': scaler_X,\n",
    "                'scaler_y': scaler_y,\n",
    "                'type': model_type,\n",
    "                'best_score': None,\n",
    "                'search_time': 0\n",
    "            }\n",
    "            \n",
    "            best_params_dict[name] = model.get_params()\n",
    "            \n",
    "            print(f\"  {name} 训练完成\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"  训练 {name} 模型时出错: {str(e)}\")\n",
    "            continue\n",
    "\n",
    "    # ========== 添加LSTM训练 ==========\n",
    "    if KERAS_AVAILABLE:\n",
    "        print(f\"\\n训练 LSTM 模型（默认参数）...\")\n",
    "        try:\n",
    "            lstm_features = select_model_specific_features(\n",
    "                working_features, \n",
    "                train_periods[working_features], \n",
    "                y_train, \n",
    "                'deep',\n",
    "                'LSTM'\n",
    "            )\n",
    "            \n",
    "            X_train_lstm = train_periods[lstm_features]\n",
    "            X_train_processed, _, y_train_processed, scaler_X, scaler_y = get_preprocessed_data(\n",
    "                X_train_lstm, X_train_lstm, y_train, 'deep'\n",
    "            )\n",
    "            \n",
    "            # 准备序列数据\n",
    "            sequence_length = min(5, len(train_periods) // 3)\n",
    "            X_sequences, y_sequences = [], []\n",
    "            \n",
    "            for i in range(sequence_length, len(X_train_processed)):\n",
    "                X_sequences.append(X_train_processed[i-sequence_length:i])\n",
    "                y_sequences.append(y_train_processed[i])\n",
    "            \n",
    "            X_sequences = np.array(X_sequences)\n",
    "            y_sequences = np.array(y_sequences)\n",
    "            \n",
    "            # 创建并训练LSTM\n",
    "            input_shape = (sequence_length, len(lstm_features))\n",
    "            lstm_model = Sequential([\n",
    "                LSTM(32, return_sequences=True, input_shape=input_shape),\n",
    "                Dropout(0.2),\n",
    "                LSTM(16, return_sequences=False),\n",
    "                Dropout(0.2),\n",
    "                Dense(8, activation='relu'),\n",
    "                Dense(1)\n",
    "            ])\n",
    "            \n",
    "            lstm_model.compile(\n",
    "                optimizer=Adam(learning_rate=0.001),\n",
    "                loss='mse',\n",
    "                metrics=['mae']\n",
    "            )\n",
    "            \n",
    "            early_stop = EarlyStopping(monitor='loss', patience=10, restore_best_weights=True)\n",
    "            reduce_lr = ReduceLROnPlateau(monitor='loss', factor=0.5, patience=5, min_lr=0.0001)\n",
    "            \n",
    "            lstm_model.fit(\n",
    "                X_sequences, y_sequences,\n",
    "                epochs=100,\n",
    "                batch_size=min(16, len(X_sequences) // 3),\n",
    "                callbacks=[early_stop, reduce_lr],\n",
    "                verbose=0\n",
    "            )\n",
    "            \n",
    "            trained_models['LSTM'] = {\n",
    "                'model': lstm_model,\n",
    "                'features': lstm_features,\n",
    "                'scaler_X': scaler_X,\n",
    "                'scaler_y': scaler_y,\n",
    "                'type': 'deep',\n",
    "                'sequence_length': sequence_length,\n",
    "                'best_score': None,\n",
    "                'search_time': 0\n",
    "            }\n",
    "            \n",
    "            best_params_dict['LSTM'] = {\n",
    "                'sequence_length': sequence_length,\n",
    "                'lstm_units': [32, 16],\n",
    "                'dropout': 0.2,\n",
    "                'learning_rate': 0.001\n",
    "            }\n",
    "            \n",
    "            print(f\"  LSTM 训练完成\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"  训练 LSTM 模型时出错: {str(e)}\")    \n",
    "    \n",
    "    return trained_models, best_params_dict\n",
    "\n",
    "\n",
    "def train_models_with_hyperparameter_search(train_periods, working_features, y_train, product_name, config,\n",
    "                                           max_time_per_model=180, search_strategy=\"smart\"):\n",
    "    \"\"\"使用超参数搜索训练模型 - 配置驱动版本\"\"\"\n",
    "    \n",
    "    print(f\"开始超参数搜索训练（策略: {search_strategy}）...\")\n",
    "    \n",
    "    # 获取搜索配置\n",
    "    models_config = get_hyperparameter_search_config()\n",
    "    \n",
    "    trained_models = {}\n",
    "    best_params_dict = {}\n",
    "    \n",
    "    for name, model_config in models_config.items():\n",
    "        print(f\"\\n训练 {name} 模型...\")\n",
    "        \n",
    "        try:\n",
    "            model_type = model_config['type']\n",
    "            \n",
    "            # 特征选择\n",
    "            model_features = select_model_specific_features(\n",
    "                working_features, \n",
    "                train_periods[working_features], \n",
    "                y_train, \n",
    "                model_type, \n",
    "                name\n",
    "            )\n",
    "            \n",
    "            print(f\"  {name} 使用 {len(model_features)} 个特征\")\n",
    "            \n",
    "            X_train = train_periods[model_features]\n",
    "            X_train_processed, _, y_train_processed, scaler_X, scaler_y = get_preprocessed_data(\n",
    "                X_train, X_train, y_train, model_type\n",
    "            )\n",
    "            \n",
    "            # 执行搜索\n",
    "            best_model, best_params, best_score, search_time = smart_hyperparameter_search(\n",
    "                model_config, X_train_processed, y_train_processed, name\n",
    "            )\n",
    "            \n",
    "            # 存储结果\n",
    "            trained_models[name] = {\n",
    "                'model': best_model,\n",
    "                'features': model_features,\n",
    "                'scaler_X': scaler_X,\n",
    "                'scaler_y': scaler_y,\n",
    "                'type': model_type,\n",
    "                'best_score': best_score,\n",
    "                'search_time': search_time\n",
    "            }\n",
    "            \n",
    "            best_params_dict[name] = best_params\n",
    "            \n",
    "            print(f\"  {name} 搜索完成，用时: {search_time:.1f}秒\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"  训练 {name} 模型时出错: {str(e)}\")\n",
    "            continue\n",
    "\n",
    "    # ========== 添加LSTM训练 ==========\n",
    "    if KERAS_AVAILABLE:\n",
    "        print(f\"\\n训练 LSTM 模型...\")\n",
    "        start_time = time.time()  # 计时开始\n",
    "        \n",
    "        try:\n",
    "            lstm_features = select_model_specific_features(\n",
    "                working_features, \n",
    "                train_periods[working_features], \n",
    "                y_train, \n",
    "                'deep',\n",
    "                'LSTM'\n",
    "            )\n",
    "            \n",
    "            print(f\"  LSTM 使用 {len(lstm_features)} 个特征\")\n",
    "            \n",
    "            X_train_lstm = train_periods[lstm_features]\n",
    "            X_train_processed, _, y_train_processed, scaler_X, scaler_y = get_preprocessed_data(\n",
    "                X_train_lstm, X_train_lstm, y_train, 'deep'\n",
    "            )\n",
    "            \n",
    "            sequence_length = min(5, len(train_periods) // 3)\n",
    "            X_sequences, y_sequences = [], []\n",
    "            \n",
    "            for i in range(sequence_length, len(X_train_processed)):\n",
    "                X_sequences.append(X_train_processed[i-sequence_length:i])\n",
    "                y_sequences.append(y_train_processed[i])\n",
    "            \n",
    "            X_sequences = np.array(X_sequences)\n",
    "            y_sequences = np.array(y_sequences)\n",
    "            \n",
    "            print(f\"  LSTM序列形状: {X_sequences.shape}\")\n",
    "            \n",
    "            input_shape = (sequence_length, len(lstm_features))\n",
    "            lstm_model = Sequential([\n",
    "                LSTM(32, return_sequences=True, input_shape=input_shape),\n",
    "                Dropout(0.2),\n",
    "                LSTM(16, return_sequences=False),\n",
    "                Dropout(0.2),\n",
    "                Dense(8, activation='relu'),\n",
    "                Dense(1)\n",
    "            ])\n",
    "            \n",
    "            lstm_model.compile(\n",
    "                optimizer=Adam(learning_rate=0.001),\n",
    "                loss='mse',\n",
    "                metrics=['mae']\n",
    "            )\n",
    "            \n",
    "            early_stop = EarlyStopping(\n",
    "                monitor='val_loss',  # 监控验证loss\n",
    "                patience=10,\n",
    "                restore_best_weights=True\n",
    "            )\n",
    "            \n",
    "            reduce_lr = ReduceLROnPlateau(\n",
    "                monitor='val_loss',\n",
    "                factor=0.5,\n",
    "                patience=5,\n",
    "                min_lr=0.0001\n",
    "            )\n",
    "            \n",
    "            history = lstm_model.fit(\n",
    "                X_sequences, y_sequences,\n",
    "                epochs=100,\n",
    "                batch_size=min(16, len(X_sequences) // 3),\n",
    "                validation_split=0.2,  # 添加验证集\n",
    "                callbacks=[early_stop, reduce_lr],\n",
    "                verbose=0\n",
    "            )\n",
    "            \n",
    "            search_time = time.time() - start_time  # 计算耗时\n",
    "            best_val_loss = min(history.history.get('val_loss', [float('inf')]))\n",
    "            \n",
    "            trained_models['LSTM'] = {\n",
    "                'model': lstm_model,\n",
    "                'features': lstm_features,\n",
    "                'scaler_X': scaler_X,\n",
    "                'scaler_y': scaler_y,\n",
    "                'type': 'deep',\n",
    "                'sequence_length': sequence_length,\n",
    "                'best_score': -best_val_loss,  # 负值转换为\"越大越好\"\n",
    "                'search_time': search_time\n",
    "            }\n",
    "            \n",
    "            best_params_dict['LSTM'] = {\n",
    "                'sequence_length': sequence_length,\n",
    "                'lstm_units': [32, 16],\n",
    "                'dropout': 0.2,\n",
    "                'learning_rate': 0.001,\n",
    "                'batch_size': min(16, len(X_sequences) // 3),\n",
    "                'final_train_loss': history.history['loss'][-1],\n",
    "                'best_val_loss': best_val_loss,\n",
    "                'epochs_trained': len(history.history['loss'])\n",
    "            }\n",
    "            \n",
    "            print(f\"  LSTM 训练完成，用时: {search_time:.1f}秒\")\n",
    "            print(f\"  最佳验证loss: {best_val_loss:.4f}\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"  训练 LSTM 模型时出错: {str(e)}\")\n",
    "            import traceback\n",
    "            traceback.print_exc()\n",
    "    \n",
    "    return trained_models, best_params_dict\n",
    "\n",
    "def fallback_to_defaults(config, X_train, y_train, model_name):\n",
    "    \"\"\"fallback到默认参数\"\"\"\n",
    "    \n",
    "    print(f\"    使用默认参数...\")\n",
    "    \n",
    "    start_time = time.time()\n",
    "    \n",
    "    model = config['base_model']\n",
    "    \n",
    "    # 使用改进的默认参数\n",
    "    if model_name == 'SVR':\n",
    "        model.set_params(kernel='rbf', C=1000, gamma='auto', epsilon=0.01)\n",
    "    elif model_name == 'XGBoost':\n",
    "        model.set_params(n_estimators=500, learning_rate=0.02, max_depth=6)\n",
    "    \n",
    "    model.fit(X_train, y_train)\n",
    "    \n",
    "    search_time = time.time() - start_time\n",
    "    \n",
    "    return model, model.get_params(), None, search_time\n",
    "\n",
    "def save_best_parameters(best_params_dict, product_name, output_dir, config):\n",
    "    \"\"\"保存最佳参数到文件\"\"\"\n",
    "    \n",
    "    if not best_params_dict:\n",
    "        return\n",
    "    \n",
    "    params_data = []\n",
    "    for model_name, params in best_params_dict.items():\n",
    "        for param_name, param_value in params.items():\n",
    "            params_data.append({\n",
    "                'model': model_name,\n",
    "                'parameter': param_name,\n",
    "                'best_value': str(param_value)\n",
    "            })\n",
    "    \n",
    "    if params_data:\n",
    "        params_df = pd.DataFrame(params_data)\n",
    "        params_path = os.path.join(output_dir, f\"{product_name}_{config.name}_最佳参数.xlsx\")\n",
    "        params_df.to_excel(params_path, index=False)\n",
    "        print(f\"最佳参数已保存至: {params_path}\")\n",
    "\n",
    "\n",
    "def smart_hyperparameter_search(config, X_train, y_train, model_name, data_threshold=50):\n",
    "    \"\"\"智能超参数搜索：根据数据量和模型类型选择搜索策略\"\"\"\n",
    "    \n",
    "    data_size = len(X_train)\n",
    "    print(f\"    数据量: {data_size}\")\n",
    "    \n",
    "    start_time = time.time()\n",
    "    \n",
    "    try:\n",
    "        # 动态调整CV折数（对于15天聚合，数据量较少）\n",
    "        cv_folds = min(3, max(2, data_size // 5))\n",
    "        tscv = TimeSeriesSplit(n_splits=cv_folds)\n",
    "        \n",
    "        # SVR强制使用网格搜索\n",
    "        if config.get('use_grid_only', False):\n",
    "            print(f\"    使用网格搜索（强制模式）\")\n",
    "            \n",
    "            search = GridSearchCV(\n",
    "                estimator=config['base_model'],\n",
    "                param_grid=config['param_grid'],\n",
    "                scoring='neg_mean_absolute_error',\n",
    "                cv=tscv,\n",
    "                n_jobs=1,\n",
    "                verbose=0\n",
    "            )\n",
    "        elif data_size < data_threshold:\n",
    "            # 小数据集：使用网格搜索\n",
    "            print(f\"    使用网格搜索（数据量 < {data_threshold}）\")\n",
    "            \n",
    "            search = GridSearchCV(\n",
    "                estimator=config['base_model'],\n",
    "                param_grid=config['param_grid'],\n",
    "                scoring='neg_mean_absolute_error',\n",
    "                cv=tscv,\n",
    "                n_jobs=1,\n",
    "                verbose=0,\n",
    "                random_state=42\n",
    "            )\n",
    "        else:\n",
    "            # 大数据集：使用随机搜索\n",
    "            print(f\"    使用随机搜索（数据量 >= {data_threshold}）\")\n",
    "            \n",
    "            search = RandomizedSearchCV(\n",
    "                estimator=config['base_model'],\n",
    "                param_distributions=config['param_distributions'],\n",
    "                n_iter=config['n_iter'],\n",
    "                scoring='neg_mean_absolute_error',\n",
    "                cv=tscv,\n",
    "                n_jobs=1,\n",
    "                verbose=0,\n",
    "                random_state=42\n",
    "            )\n",
    "        \n",
    "        search.fit(X_train, y_train)\n",
    "        \n",
    "        search_time = time.time() - start_time\n",
    "        \n",
    "        print(f\"    搜索完成，用时: {search_time:.1f}秒\")\n",
    "        print(f\"    最佳参数: {search.best_params_}\")\n",
    "        print(f\"    最佳得分: {search.best_score_:.4f}\")\n",
    "        \n",
    "        return search.best_estimator_, search.best_params_, search.best_score_, search_time\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"    智能搜索失败: {str(e)}\")\n",
    "        return fallback_to_defaults(config, X_train, y_train, model_name)\n",
    "\n",
